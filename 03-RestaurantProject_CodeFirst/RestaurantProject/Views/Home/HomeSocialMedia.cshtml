@model List<RestaurantProject.Models.RestaurantSocialMedia>
@{
    ViewBag.Title = "HomeSocialMedia";
}


<div class="col-lg-3 col-md-6">
    <h4>Follow Us</h4>
    <div class="social-links d-flex">
        @foreach (var socialMedia in Model)
        {
            <a href="@socialMedia.Url" ><i class="bi @socialMedia.Icon"></i></a>
        }
    </div>
</div>